{
    "version": 2,
    "public": true,
    "builds": [
     
      {
        "src": "derat/package.json",
        "use": "@vercel/static-build"
      },
      { "src": "derat/api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
   
      {
        "src": "/(.*)",
        "dest": "derat/$1"
      }
    ]
  }